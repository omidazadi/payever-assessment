{
    "moduleFileExtensions": ["js", "json", "ts"],
    "moduleDirectories": ["node_modules", "<rootDir>"],
    "rootDir": "../..",
    "testEnvironment": "node",
    "testMatch": ["<rootDir>/test/integration/**/*.spec.ts"],
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFiles": ["./test/integration/setup-file.ts"]
}
